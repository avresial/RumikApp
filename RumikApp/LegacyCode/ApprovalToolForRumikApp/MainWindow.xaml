<Window x:Class="ApprovalToolForRumikApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:UserControls="clr-namespace:ApprovalToolForRumikApp.UserControls"
        xmlns:local="clr-namespace:ApprovalToolForRumikApp"
        mc:Ignorable="d"
        WindowState="Maximized" WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <UserControls:InsertDataToDatabaseForm Grid.Row="0" Grid.ColumnSpan="2" DataContext="{Binding InsertDataToDatabaseFromViewModel}"/>
    </Grid>
</Window>
